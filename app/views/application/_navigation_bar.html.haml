    .collapse.navbar-collapse
      %ul.nav.navbar-nav
        %li= link_to "Carl Hinderer's Blog", root_url, {class: 'navbar-brand'}
        %li= link_to 'Programming Blog', '/posts/category/programming', {class: 'navbar_option'}
        %li= link_to 'Personal Blog', '/posts/category/personal', {class: 'navbar_option' }
